{
    "name": "elgibor-solution/laravel-loki-logging",
    "version": "1.0.0",
    "description": "Logging to Loki for Laravel (PHP 8)",
    "type": "library",
    "authors": [
        {
            "name": "E-Solution",
            "email": "tommy@elgibor-solution.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "illuminate/support": "^8 || ^9",
        "illuminate/console": "^8 || ^9",
        "monolog/monolog": "^2"
    },
    "autoload": {
        "psr-4": {
            "ESolution\\LaravelLokiLogging\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ESolution\\LaravelLokiLogging\\L3ServiceProvider"
            ]
        }
    }
}
